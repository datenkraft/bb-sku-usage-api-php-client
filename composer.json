{
    "name": "datenkraft/bb-sku-usage-api-php-client",
    "description": "The SKU Usage API PHP Client enables you to work with the SKU Usage API",
    "type": "library",
    "require": {
        "php": "^7.2",
        "datenkraft/bb-base-api-php-client": "@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Datenkraft GmbH",
            "email": "office@datenkraft.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Datenkraft\\Backbone\\Client\\SkuUsageApi\\": "src/"
        }
    },
    "require-dev": {
      "phpunit/phpunit": "^9.5",
      "pact-foundation/pact-php": "^7.0",
      "squizlabs/php_codesniffer": "^3.6",
        "friendsofphp/php-cs-fixer": "^2.18",
        "jane-php/open-api-3": "^6.3"
    },
    "autoload-dev": {
      "classmap": [
        "tests/"
      ]
    }
}
