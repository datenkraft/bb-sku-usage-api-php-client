{
    "name": "datenkraft/bb-sku-usage-api-php-client",
    "description": "The SKU Usage API PHP Client enables you to work with the SKU Usage API",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.3",
        "jane-php/open-api-runtime": "~6.1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Datenkraft",
            "email": "office@datenkraft.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Datenkraft\\bb-sku-usage-api-php-client\\": "src/"
        }
    },
    "require-dev": {
      "phpunit/phpunit": "^9.5",
      "pact-foundation/pact-php": "^7.0",
      "squizlabs/php_codesniffer": "^3.6",
      "ext-json": "^7.4",
      "jane-php/open-api-3": "~6.1.0"
    },
    "autoload-dev": {
      "classmap": [
        "tests/"
      ]
    }
}
